{
  "name": "demo",
  "version": "1.0.0",
  "description": "Demo project",
  "scripts": {
    "tscheck": "nx run-many --all --target=tscheck",
    "format": "nx format:write",
    "build:all": "nx run-many --all --target=build",
    "build:email-templates": "mkdir -p dist/email-templates && mjml apps/email-service/src/app/templates/*.mjml -o dist/email-templates && cp apps/email-service/src/app/templates/*.txt dist/email-templates/",
    "test:all": "nx run-many --all --target=test --coverage",
    "lint:all": "nx run-many --all --target=lint",
    "check-all": "nx run-many --all --targets=lint,tscheck,test",
    "prepare": "husky install",
    "start:demo": "yarn nx run-many --parallel --target=serve --projects=client-mx,server-nest,auth-service,email-service",
    "test:e2e": "nx run-many --all --target=e2e --parallel",
    "prisma:generate": "prisma generate --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "prisma:migrate:reset": "prisma migrate reset --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "prisma:studio": "prisma studio --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "prisma:db:push": "prisma db push --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "seed": "npx prisma db seed --schema=./apps/auth-service/src/prisma-setup/schema.prisma",
    "analyze:client": "ANALYZE=true yarn nx build client-mx --configuration=production && node tools/build/analyze-bundle.js"
  },
  "author": "Oleksiy Petrychko",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} apps/auth-service/src/prisma-setup/seed/seed.ts"
  },
